		jQuery(function() {
			jQuery( "#btn" ).button();
			
			jQuery( "#datepicker" ).datepicker
		});
		
		function validar(){  
			if(jQuery('#txtNombre').val().length < 1) {   
				jQuery( "#mensaje2" ).dialog(); 
			}
			else if(jQuery("#txtComida").val().length < 1){
				jQuery( "#mensaje2" ).dialog(); 
			}
			else if(jQuery("#txtTelefono").val().length < 1){
				jQuery( "#mensaje2" ).dialog(); 
			}
			else if(jQuery("#txtUbicacion").val().length < 1){
				jQuery( "#mensaje2" ).dialog(); 
			}
			else { 				
				jQuery("#dialog").dialog({
					resizable: false,
					height: 200,
					width: 300,
					modal: true,
					buttons: {
						"Ok": function () {
							jQuery("#formulario1").submit();
						},
				}
				});
			}
		}
		
	</script>		
